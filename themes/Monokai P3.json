  "tokenColors": [
    {
      "name": "Monokai: Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#75715e", "fontStyle": "italic" }
    },
    {
      "name": "Monokai: Keywords / Control Flow (Laser Rose)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#ff005f" }
    },
    {
      "name": "Monokai: Types",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#00e5ff" }
    },
    {
      "name": "Monokai: Functions (Liquid Cyan)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#00e5ff" }
    },
    {
      "name": "Monokai: Strings (Limelight)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#a6ff00" }
    },
    {
      "name": "Monokai: Numbers / Constants (Electric Orchid)",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "support.constant"
      ],
      "settings": { "foreground": "#ae81ff" }
    },
    {
      "name": "Monokai: Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#f8f8f2" }
    },
    {
      "name": "Monokai: Parameters / Attributes (Safety Orange)",
      "scope": [
        "variable.parameter",
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#ff9500" }
    },
    {
      "name": "Monokai: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#ff9500" }
    },
    {
      "name": "Monokai: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#bdbdbd" }
    },
    {
      "name": "Monokai: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff005f", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#00e5ff",
    "type": "#00e5ff",
    "typeParameter": "#ff9500",
    "class": "#00e5ff",
    "interface": "#00e5ff",
    "enum": "#00e5ff",
    "enumMember": "#ae81ff",
    "function": "#00e5ff",
    "method": "#00e5ff",
    "macro": "#ff005f",
    "parameter": "#ff9500",
    "property": "#ff9500",
    "variable": "#f8f8f2",
    "decorator": "#ff9500",
    "comment": {
      "foreground": "#75715e",
      "italic": true
    }
  }
}
