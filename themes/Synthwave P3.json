  "tokenColors": [
    {
      "name": "Synthwave: Comments (Acid Lime)",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#adff2f", "fontStyle": "italic" }
    },
    {
      "name": "Synthwave: Keywords (Laser Magenta)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#ff00ff" }
    },
    {
      "name": "Synthwave: Types / Classes (Fusion Gold)",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#fff200" }
    },
    {
      "name": "Synthwave: Functions / Methods (Electric Sky)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#00d9ff" }
    },
    {
      "name": "Synthwave: Strings (Sunset Glow)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#ff8c00" }
    },
    {
      "name": "Synthwave: Constants / Numbers (Sunset Glow)",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "support.constant"
      ],
      "settings": { "foreground": "#ff8c00" }
    },
    {
      "name": "Synthwave: Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#d7c0ff" }
    },
    {
      "name": "Synthwave: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#ffd6a6" }
    },
    {
      "name": "Synthwave: Object Properties (Laser Magenta)",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#ff00ff" }
    },
    {
      "name": "Synthwave: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#cbb6ea" }
    },
    {
      "name": "Synthwave: Attributes / Decorators (Fusion Gold)",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#fff200" }
    },
    {
      "name": "Synthwave: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff4d7d", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#fff200",
    "type": "#fff200",
    "typeParameter": "#ffd6a6",
    "class": "#fff200",
    "interface": "#fff200",
    "enum": "#fff200",
    "enumMember": "#ff8c00",
    "function": "#00d9ff",
    "method": "#00d9ff",
    "macro": "#ff00ff",
    "parameter": "#ffd6a6",
    "property": "#ff00ff",
    "variable": "#d7c0ff",
    "decorator": "#fff200",
    "comment": {
      "foreground": "#adff2f",
      "italic": true
    }
  }
}
