  "tokenColors": [
    {
      "name": "Hyper-Vapor: Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#8e79b3", "fontStyle": "italic" }
    },
    {
      "name": "Hyper-Vapor: Keywords (Laser Magenta)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#ff00f2" }
    },
    {
      "name": "Hyper-Vapor: Types",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#66eaff" }
    },
    {
      "name": "Hyper-Vapor: Functions (Lemonade)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#d4ff00" }
    },
    {
      "name": "Hyper-Vapor: Strings (Plasma Blue)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#00d9ff" }
    },
    {
      "name": "Hyper-Vapor: Numbers / Constants",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "support.constant"
      ],
      "settings": { "foreground": "#ffd86b" }
    },
    {
      "name": "Hyper-Vapor: Variables (Peach)",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#ff9d80" }
    },
    {
      "name": "Hyper-Vapor: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#ffc6b3" }
    },
    {
      "name": "Hyper-Vapor: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#00d9ff" }
    },
    {
      "name": "Hyper-Vapor: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#c1a6ee" }
    },
    {
      "name": "Hyper-Vapor: Attributes / Decorators",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#ff00f2" }
    },
    {
      "name": "Hyper-Vapor: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff4d7d", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#66eaff",
    "type": "#66eaff",
    "typeParameter": "#ffc6b3",
    "class": "#66eaff",
    "interface": "#ffc6b3",
    "enum": "#66eaff",
    "enumMember": "#ffd86b",
    "function": "#d4ff00",
    "method": "#d4ff00",
    "macro": "#ff00f2",
    "parameter": "#ffc6b3",
    "property": "#00d9ff",
    "variable": "#ff9d80",
    "decorator": "#ff00f2",
    "comment": {
      "foreground": "#8e79b3",
      "italic": true
    }
  }
}
