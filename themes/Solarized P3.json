  "tokenColors": [
    {
      "name": "Solarized: Comments (Muted Violet)",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#8b7a9a", "fontStyle": "italic" }
    },
    {
      "name": "Solarized: Keywords (P3 Burnt Orange)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#ff4d00" }
    },
    {
      "name": "Solarized: Types",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#b58cff" }
    },
    {
      "name": "Solarized: Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#33c4ff" }
    },
    {
      "name": "Solarized: Strings (P3 Soft Mint)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#66ffb3" }
    },
    {
      "name": "Solarized: Numbers",
      "scope": ["constant.numeric", "constant.language.boolean", "constant.language.null"],
      "settings": { "foreground": "#ffd27a" }
    },
    {
      "name": "Solarized: Constants",
      "scope": [
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "constant.other.color",
        "support.constant"
      ],
      "settings": { "foreground": "#ff8a33" }
    },
    {
      "name": "Solarized: Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#ffcc66" }
    },
    {
      "name": "Solarized: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#ffe0a3" }
    },
    {
      "name": "Solarized: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#4dffe1" }
    },
    {
      "name": "Solarized: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#b3a9a2" }
    },
    {
      "name": "Solarized: Attributes / Decorators",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#ff8a33" }
    },
    {
      "name": "Solarized: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff3b30", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#33c4ff",
    "type": "#b58cff",
    "typeParameter": "#d0b8ff",
    "class": "#b58cff",
    "interface": "#d0b8ff",
    "enum": "#b58cff",
    "enumMember": "#ffcc66",
    "function": "#33c4ff",
    "method": "#33c4ff",
    "macro": "#ff8a33",
    "parameter": "#ffe0a3",
    "property": "#4dffe1",
    "variable": "#ffcc66",
    "decorator": "#ff8a33",
    "comment": {
      "foreground": "#8b7a9a",
      "italic": true
    }
  }
}
