  "tokenColors": [
    {
      "name": "Midnight Press: Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6f7276", "fontStyle": "italic" }
    },
    {
      "name": "Midnight Press: Keywords (Pigment Violet)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "entity.name.function.preprocessor",
        "meta.preprocessor",
        "keyword.control.directive",
        "support.other.directive"
      ],
      "settings": { "foreground": "#6b00b8" }
    },
    {
      "name": "Midnight Press: Types / Classes",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#cfd0d0" }
    },
    {
      "name": "Midnight Press: Functions / Methods (Cadmium Orange)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#d65d00" }
    },
    {
      "name": "Midnight Press: Strings (Cobalt Teal)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#008282" }
    },
    {
      "name": "Midnight Press: Numbers / Constants (Ultramarine)",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "support.constant"
      ],
      "settings": { "foreground": "#0033ff" }
    },
    {
      "name": "Midnight Press: Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#d7d5d2" }
    },
    {
      "name": "Midnight Press: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#d65d00" }
    },
    {
      "name": "Midnight Press: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#008282" }
    },
    {
      "name": "Midnight Press: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#a9adb0" }
    },
    {
      "name": "Midnight Press: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff5a6a", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#cfd0d0",
    "type": "#cfd0d0",
    "typeParameter": "#d65d00",
    "class": "#cfd0d0",
    "interface": "#cfd0d0",
    "enum": "#cfd0d0",
    "enumMember": "#0033ff",
    "function": "#d65d00",
    "method": "#d65d00",
    "macro": "#6b00b8",
    "parameter": "#d65d00",
    "property": "#008282",
    "variable": "#d7d5d2",
    "decorator": "#6b00b8",
    "comment": {
      "foreground": "#6f7276",
      "italic": true
    }
  }
}
