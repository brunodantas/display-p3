  "tokenColors": [
    {
      "name": "One Dark: Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#7f8694", "fontStyle": "italic" }
    },
    {
      "name": "One Dark: Keywords / Macros (Sharp Cherry)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "entity.name.function.preprocessor",
        "meta.preprocessor",
        "keyword.control.directive",
        "support.other.directive"
      ],
      "settings": { "foreground": "#ff4d5a" }
    },
    {
      "name": "One Dark: Types / Classes (Electric Amethyst)",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#d55fff" }
    },
    {
      "name": "One Dark: Functions / Methods (Clear Azure)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#00aaff" }
    },
    {
      "name": "One Dark: Strings (Glassy Green)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#00ff88" }
    },
    {
      "name": "One Dark: Constants / Numbers / Booleans (Liquid Gold)",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "support.constant"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "One Dark: Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#cfd7e6" }
    },
    {
      "name": "One Dark: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "One Dark: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#00aaff" }
    },
    {
      "name": "One Dark: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#aab3c0" }
    },
    {
      "name": "One Dark: Attributes / Decorators",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "One Dark: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff4d5a", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#d55fff",
    "type": "#d55fff",
    "typeParameter": "#ffcc00",
    "class": "#d55fff",
    "interface": "#d55fff",
    "enum": "#d55fff",
    "enumMember": "#ffcc00",
    "function": "#00aaff",
    "method": "#00aaff",
    "macro": "#ff4d5a",
    "parameter": "#ffcc00",
    "property": "#00aaff",
    "variable": "#cfd7e6",
    "decorator": "#ffcc00",
    "comment": {
      "foreground": "#7f8694",
      "italic": true
    }
  }
}
