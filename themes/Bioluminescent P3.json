  "tokenColors": [
    {
      "name": "Bioluminescent: Comments (Driftwood)",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#5c6b73", "fontStyle": "italic" }
    },
    {
      "name": "Bioluminescent: Keywords (P3 Sea-Slug Purple)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "entity.name.function.preprocessor",
        "meta.preprocessor",
        "keyword.control.directive",
        "support.other.directive"
      ],
      "settings": { "foreground": "#d400ff" }
    },
    {
      "name": "Bioluminescent: Types / Classes",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#6aff00" }
    },
    {
      "name": "Bioluminescent: Functions / Methods (P3 Anemone Pink)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#ff007a" }
    },
    {
      "name": "Bioluminescent: Strings (P3 Jellyfish Teal)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#00ffd5" }
    },
    {
      "name": "Bioluminescent: Numbers / Constants",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "support.constant"
      ],
      "settings": { "foreground": "#ffe66d" }
    },
    {
      "name": "Bioluminescent: Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#d8e2ff" }
    },
    {
      "name": "Bioluminescent: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#ff007a" }
    },
    {
      "name": "Bioluminescent: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#00ffd5" }
    },
    {
      "name": "Bioluminescent: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#a8b7db" }
    },
    {
      "name": "Bioluminescent: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff5a6a", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#6aff00",
    "type": "#6aff00",
    "typeParameter": "#ff007a",
    "class": "#6aff00",
    "interface": "#6aff00",
    "enum": "#6aff00",
    "enumMember": "#ffe66d",
    "function": "#ff007a",
    "method": "#ff007a",
    "macro": "#d400ff",
    "parameter": "#ff007a",
    "property": "#00ffd5",
    "variable": "#d8e2ff",
    "decorator": "#d400ff",
    "comment": {
      "foreground": "#5c6b73",
      "italic": true
    }
  }
}
