  "tokenColors": [
    {
      "name": "Solar Flare: Comments (Desaturated Bronze)",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#7d6b5d", "fontStyle": "italic" }
    },
    {
      "name": "Solar Flare: Keywords (Supernova Orange)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#ff4d00" }
    },
    {
      "name": "Solar Flare: Types",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#7dd7ff" }
    },
    {
      "name": "Solar Flare: Functions (Rosewood)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#ff0055" }
    },
    {
      "name": "Solar Flare: Strings (Sun-Glow)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "Solar Flare: Numbers / Constants",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "support.constant"
      ],
      "settings": { "foreground": "#ff6a2c" }
    },
    {
      "name": "Solar Flare: Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#f4e9df" }
    },
    {
      "name": "Solar Flare: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#ffe6c7" }
    },
    {
      "name": "Solar Flare: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "Solar Flare: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#c5ad99" }
    },
    {
      "name": "Solar Flare: Attributes / Decorators",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#ff4d00" }
    },
    {
      "name": "Solar Flare: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff0055", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#7dd7ff",
    "type": "#7dd7ff",
    "typeParameter": "#ffe6c7",
    "class": "#7dd7ff",
    "interface": "#ffe6c7",
    "enum": "#7dd7ff",
    "enumMember": "#ff6a2c",
    "function": "#ff0055",
    "method": "#ff0055",
    "macro": "#ff4d00",
    "parameter": "#ffe6c7",
    "property": "#ffcc00",
    "variable": "#f4e9df",
    "decorator": "#ff4d00",
    "comment": {
      "foreground": "#7d6b5d",
      "italic": true
    }
  }
}
