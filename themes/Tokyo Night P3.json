  "tokenColors": [
    {
      "name": "Tokyo Night: Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#5a6b88", "fontStyle": "italic" }
    },
    {
      "name": "Tokyo Night: Keywords (Cyber Orchid)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#ff00ff" }
    },
    {
      "name": "Tokyo Night: Types / Classes",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#00a6ff" }
    },
    {
      "name": "Tokyo Night: Functions / Methods (Shinjuku Electric)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#00f0ff" }
    },
    {
      "name": "Tokyo Night: Strings (Neon Moss)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#00ff9d" }
    },
    {
      "name": "Tokyo Night: Constants / Numbers / Tags (Signal Flare)",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "support.constant",
        "entity.name.tag",
        "entity.name.tag.html",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": { "foreground": "#ff4d4d" }
    },
    {
      "name": "Tokyo Night: Variables (Electric Cerulean)",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#00a6ff" }
    },
    {
      "name": "Tokyo Night: Parameters / Booleans (Golden Hour)",
      "scope": ["variable.parameter", "constant.language.boolean"],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "Tokyo Night: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#00a6ff" }
    },
    {
      "name": "Tokyo Night: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#9aa5d1" }
    },
    {
      "name": "Tokyo Night: Attributes / Decorators",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "Tokyo Night: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff4d4d", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#00a6ff",
    "type": "#00a6ff",
    "typeParameter": "#ffcc00",
    "class": "#00a6ff",
    "interface": "#00a6ff",
    "enum": "#00a6ff",
    "enumMember": "#ff4d4d",
    "function": "#00f0ff",
    "method": "#00f0ff",
    "macro": "#ff00ff",
    "parameter": "#ffcc00",
    "property": "#00a6ff",
    "variable": "#00a6ff",
    "decorator": "#ffcc00",
    "comment": {
      "foreground": "#5a6b88",
      "italic": true
    }
  }
}
