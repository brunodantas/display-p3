  "tokenColors": [
    {
      "name": "Neon P3: Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6b768b", "fontStyle": "italic" }
    },
    {
      "name": "Neon P3: Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#00ffff" }
    },
    {
      "name": "Neon P3: Types",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#9d66ff" }
    },
    {
      "name": "Neon P3: Functions / Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#80ff00" }
    },
    {
      "name": "Neon P3: Strings",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#ff0080" }
    },
    {
      "name": "Neon P3: Numbers",
      "scope": ["constant.numeric", "constant.language.boolean", "constant.language.null"],
      "settings": { "foreground": "#ffff66" }
    },
    {
      "name": "Neon P3: Constants",
      "scope": [
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "constant.other.color",
        "support.constant"
      ],
      "settings": { "foreground": "#00aaff" }
    },
    {
      "name": "Neon P3: Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#ff8800" }
    },
    {
      "name": "Neon P3: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#ffb000" }
    },
    {
      "name": "Neon P3: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#33ffff" }
    },
    {
      "name": "Neon P3: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#aab2c0" }
    },
    {
      "name": "Neon P3: Attributes / Decorators",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#ff8800" }
    },
    {
      "name": "Neon P3: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff0080", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#00aaff",
    "type": "#9d66ff",
    "typeParameter": "#cc99ff",
    "class": "#9d66ff",
    "interface": "#b58cff",
    "enum": "#9d66ff",
    "enumMember": "#00aaff",
    "function": "#80ff00",
    "method": "#80ff00",
    "macro": "#ff8800",
    "parameter": "#ffb000",
    "property": "#33ffff",
    "variable": "#ff8800",
    "decorator": "#ff8800",
    "comment": {
      "foreground": "#6b768b",
      "italic": true
    }
  }
}
