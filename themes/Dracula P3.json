  "tokenColors": [
    {
      "name": "Dracula: Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6272a4", "fontStyle": "italic" }
    },
    {
      "name": "Dracula: Keywords / Control Flow (Laser Orchid)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#ff00aa" }
    },
    {
      "name": "Dracula: Types / Classes (Electric Violet)",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#bd93f9" }
    },
    {
      "name": "Dracula: Functions / Methods (Glacial Blue)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#00f0ff" }
    },
    {
      "name": "Dracula: Strings (Radioactive Lime)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#00ff80" }
    },
    {
      "name": "Dracula: Attributes / Decorators (Solar Gold)",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#ffff00" }
    },
    {
      "name": "Dracula: Numbers / Constants (Magma)",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "support.constant"
      ],
      "settings": { "foreground": "#ff9500" }
    },
    {
      "name": "Dracula: Variables (Electric Violet)",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#bd93f9" }
    },
    {
      "name": "Dracula: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#ffb86c" }
    },
    {
      "name": "Dracula: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#ffff00" }
    },
    {
      "name": "Dracula: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#b7b9cc" }
    },
    {
      "name": "Dracula: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff5555", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#bd93f9",
    "type": "#bd93f9",
    "typeParameter": "#ffb86c",
    "class": "#bd93f9",
    "interface": "#bd93f9",
    "enum": "#bd93f9",
    "enumMember": "#ff9500",
    "function": "#00f0ff",
    "method": "#00f0ff",
    "macro": "#ff00aa",
    "parameter": "#ffb86c",
    "property": "#ffff00",
    "variable": "#bd93f9",
    "decorator": "#ffff00",
    "comment": {
      "foreground": "#6272a4",
      "italic": true
    }
  }
}
