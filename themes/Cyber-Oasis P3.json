  "tokenColors": [
    {
      "name": "Cyber-Oasis: Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6b8f96", "fontStyle": "italic" }
    },
    {
      "name": "Cyber-Oasis: Keywords (Electric Mint)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#00ffa3" }
    },
    {
      "name": "Cyber-Oasis: Types",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#00e5ff" }
    },
    {
      "name": "Cyber-Oasis: Functions (Violet)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#bd00ff" }
    },
    {
      "name": "Cyber-Oasis: Strings (Deep Sea Blue)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#0077ff" }
    },
    {
      "name": "Cyber-Oasis: Numbers / Constants (Coral)",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "constant.other.color",
        "support.constant"
      ],
      "settings": { "foreground": "#ff5f5f" }
    },
    {
      "name": "Cyber-Oasis: Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#7dffec" }
    },
    {
      "name": "Cyber-Oasis: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#b7ffe9" }
    },
    {
      "name": "Cyber-Oasis: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#33c4ff" }
    },
    {
      "name": "Cyber-Oasis: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#89b9c0" }
    },
    {
      "name": "Cyber-Oasis: Attributes / Decorators",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#00ffa3" }
    },
    {
      "name": "Cyber-Oasis: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff5f5f", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#00e5ff",
    "type": "#00e5ff",
    "typeParameter": "#7dffec",
    "class": "#00e5ff",
    "interface": "#7dffec",
    "enum": "#00e5ff",
    "enumMember": "#ff5f5f",
    "function": "#bd00ff",
    "method": "#bd00ff",
    "macro": "#00ffa3",
    "parameter": "#b7ffe9",
    "property": "#33c4ff",
    "variable": "#7dffec",
    "decorator": "#00ffa3",
    "comment": {
      "foreground": "#6b8f96",
      "italic": true
    }
  }
}
