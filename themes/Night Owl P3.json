  "tokenColors": [
    {
      "name": "Night Owl: Comments (Acid Lime)",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#adff2f", "fontStyle": "italic" }
    },
    {
      "name": "Night Owl: Keywords (Ultraviolet)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#ff00ff" }
    },
    {
      "name": "Night Owl: Types / Classes",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "Night Owl: Functions / Methods (Golden Amber)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "Night Owl: Strings (Bioluminescent Mint)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#00ffcc" }
    },
    {
      "name": "Night Owl: Numbers / Constants (Burnt Neon)",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "support.constant"
      ],
      "settings": { "foreground": "#ff5800" }
    },
    {
      "name": "Night Owl: Variables (Electric Cerulean)",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#00a6ff" }
    },
    {
      "name": "Night Owl: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#7ad1ff" }
    },
    {
      "name": "Night Owl: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#00a6ff" }
    },
    {
      "name": "Night Owl: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#9db6d8" }
    },
    {
      "name": "Night Owl: Attributes / Decorators",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "Night Owl: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff5800", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#ffcc00",
    "type": "#ffcc00",
    "typeParameter": "#7ad1ff",
    "class": "#ffcc00",
    "interface": "#ffcc00",
    "enum": "#ffcc00",
    "enumMember": "#ff5800",
    "function": "#ffcc00",
    "method": "#ffcc00",
    "macro": "#ff00ff",
    "parameter": "#7ad1ff",
    "property": "#00a6ff",
    "variable": "#00a6ff",
    "decorator": "#ffcc00",
    "comment": {
      "foreground": "#adff2f",
      "italic": true
    }
  }
}
