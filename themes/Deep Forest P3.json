  "tokenColors": [
    {
      "name": "Deep Forest: Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#7fa59a", "fontStyle": "italic" }
    },
    {
      "name": "Deep Forest: Keywords (P3 Gold)",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "Deep Forest: Types",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "entity.name.struct",
        "entity.name.trait",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#4dffe1" }
    },
    {
      "name": "Deep Forest: Functions (P3 Sky)",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method",
        "support.method",
        "variable.function"
      ],
      "settings": { "foreground": "#00b3ff" }
    },
    {
      "name": "Deep Forest: Strings (P3 Emerald)",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted",
        "string.template",
        "string.regexp"
      ],
      "settings": { "foreground": "#00cc66" }
    },
    {
      "name": "Deep Forest: Numbers",
      "scope": ["constant.numeric", "constant.language.boolean", "constant.language.null"],
      "settings": { "foreground": "#ffd27a" }
    },
    {
      "name": "Deep Forest: Constants",
      "scope": [
        "constant.language",
        "constant.character.escape",
        "constant.other",
        "constant.other.color",
        "support.constant"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "Deep Forest: Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": { "foreground": "#a8ffcf" }
    },
    {
      "name": "Deep Forest: Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#d6f27a" }
    },
    {
      "name": "Deep Forest: Properties / Fields",
      "scope": [
        "variable.other.property",
        "variable.other.member",
        "support.variable.property",
        "meta.object-literal.key",
        "meta.object.member"
      ],
      "settings": { "foreground": "#7dffec" }
    },
    {
      "name": "Deep Forest: Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.typeparameters"
      ],
      "settings": { "foreground": "#9fc8bb" }
    },
    {
      "name": "Deep Forest: Attributes / Decorators",
      "scope": [
        "meta.annotation",
        "punctuation.definition.annotation",
        "entity.name.function.decorator",
        "meta.decorator"
      ],
      "settings": { "foreground": "#ffcc00" }
    },
    {
      "name": "Deep Forest: Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff4d6d", "fontStyle": "underline" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#00b3ff",
    "type": "#4dffe1",
    "typeParameter": "#7dffec",
    "class": "#4dffe1",
    "interface": "#7dffec",
    "enum": "#4dffe1",
    "enumMember": "#ffcc00",
    "function": "#00b3ff",
    "method": "#00b3ff",
    "macro": "#ffcc00",
    "parameter": "#d6f27a",
    "property": "#7dffec",
    "variable": "#a8ffcf",
    "decorator": "#ffcc00",
    "comment": {
      "foreground": "#7fa59a",
      "italic": true
    }
  }
}
